<script>
function nav(id){
  document.querySelectorAll('.ps').forEach(function(s){s.classList.remove('active');});
  var t=document.getElementById(id);
  if(t){t.classList.add('active');window.scrollTo({top:0,behavior:'instant'});}
  var m=document.getElementById('mnav');
  m.classList.remove('open');
  document.getElementById('mbtn').setAttribute('aria-expanded','false');
}
document.getElementById('mbtn').addEventListener('click',function(){
  var m=document.getElementById('mnav');
  var o=m.classList.toggle('open');
  this.setAttribute('aria-expanded',String(o));
});
function toggleFaq(btn){
  var item=btn.closest('.fi-item');
  var was=item.classList.contains('active');
  document.querySelectorAll('.fi-item').forEach(function(i){
    i.classList.remove('active');
    i.querySelector('button').setAttribute('aria-expanded','false');
  });
  if(!was){item.classList.add('active');btn.setAttribute('aria-expanded','true');}
}
function handleForm(e){
  e.preventDefault();
  document.getElementById('contactForm').style.display='none';
  document.getElementById('formSuccess').style.display='block';
}
var yr=document.getElementById('yr');
if(yr){yr.textContent=new Date().getFullYear();}
</script>
