/* â”€â”€ Tokens â”€â”€ */
:root{--p:#4d7d55;--s:#4F4A45;--bg:#FDFBF7;--gl:#f0f4f1;--hbg:#f4f7f4;--fp:#9ab89c}
/* â”€â”€ Reset â”€â”€ */
*,*::before,*::after{box-sizing:border-box;margin:0;padding:0}
html{scroll-behavior:smooth}
/* â”€â”€ Base â”€â”€ */
body{font-family:'Nunito',sans-serif;background:var(--bg);color:var(--s);min-height:100vh;display:flex;flex-direction:column;line-height:1.6}
h1,h2,h3{line-height:1.2}
/* â”€â”€ Utilities â”€â”€ */
.sr-only{position:absolute;width:1px;height:1px;padding:0;margin:-1px;overflow:hidden;clip:rect(0,0,0,0);white-space:nowrap;border:0}
/* â”€â”€ Button â”€â”€ */
.btn{background:var(--p);color:#fff;border:none;cursor:pointer;font-family:inherit;transition:filter .3s,transform .3s}
.btn:hover{filter:brightness(88%);transform:translateY(-1px)}
.btn:focus-visible{outline:3px solid var(--s);outline-offset:3px}
/* â”€â”€ Animations â”€â”€ */
@media(prefers-reduced-motion:no-preference){.fade-in{animation:fi .5s ease-in forwards}@keyframes fi{from{opacity:0;transform:translateY(10px)}to{opacity:1;transform:translateY(0)}}}
/* â”€â”€ SPA routing â”€â”€ */
.ps{display:none}.ps.active{display:block}
/* â”€â”€ Accordion â”€â”€ */
.fc{max-height:0;overflow:hidden;transition:max-height .3s ease-out}
.fi-item.active .fc{max-height:500px}
.fi-icon{transition:transform .3s;display:inline-block}
.fi-item.active .fi-icon{transform:rotate(180deg)}
/* â”€â”€ Header â”€â”€ */
.hdr{position:sticky;top:0;z-index:50;background:rgba(255,255,255,.92);backdrop-filter:blur(12px);-webkit-backdrop-filter:blur(12px);box-shadow:0 1px 3px rgba(0,0,0,.06);border-bottom:1px solid #f3f4f6}
.hdr-inner{display:flex;justify-content:space-between;align-items:center;padding-block:1rem;max-width:80rem;margin-inline:auto;padding-inline:1rem}
.logo{display:flex;align-items:center;cursor:pointer;background:none;border:none;font-family:inherit;text-decoration:none}
.logo-ic{width:2.5rem;height:2.5rem;background:var(--p);border-radius:50%;display:flex;align-items:center;justify-content:center;margin-right:.75rem;flex-shrink:0}
.logo-tx{font-size:1.25rem;font-weight:700;letter-spacing:-.025em;color:var(--s)}
.dnav{display:none;align-items:center;gap:1.5rem}
.dnav a{color:var(--s);text-decoration:none;font-weight:600;transition:color .2s}
.dnav a:hover{color:var(--p)}
.dnav a:focus-visible,.mnav a:focus-visible{outline:2px solid var(--p);border-radius:4px}
.mbtn{display:flex;align-items:center;justify-content:center;padding:.5rem;background:none;border:none;cursor:pointer;color:var(--s)}
.mbtn:focus-visible{outline:2px solid var(--p);border-radius:4px}
.mnav{display:none;background:#fff;border-bottom:1px solid #f3f4f6;padding:1rem}
.mnav.open{display:block}
.mnav a{display:block;padding:.5rem 0;color:var(--s);text-decoration:none;font-weight:600}
/* â”€â”€ Hero â”€â”€ */
.hero{background:linear-gradient(to bottom,var(--hbg),transparent);padding:5rem 1rem;text-align:center}
.hero h1{font-size:clamp(2.25rem,6vw,3.75rem);font-weight:900;margin-bottom:1.5rem}
/* â”€â”€ Layout â”€â”€ */
.ctr{max-width:80rem;margin-inline:auto;padding-inline:1rem}
/* â”€â”€ Card â”€â”€ */
.card{background:#fff;border-radius:1rem;box-shadow:0 1px 3px rgba(0,0,0,.07);border:1px solid #f3f4f6}
.card-hov{transition:box-shadow .2s}.card-hov:hover{box-shadow:0 4px 16px rgba(0,0,0,.1)}
/* â”€â”€ Timeline dot â”€â”€ */
.tdot{z-index:10;width:3rem;height:3rem;background:var(--p);color:#fff;border-radius:50%;display:flex;align-items:center;justify-content:center;font-weight:700;font-size:1.25rem;box-shadow:0 4px 12px rgba(0,0,0,.15);outline:8px solid var(--bg);flex-shrink:0}
.tline{display:none;position:absolute;left:50%;top:0;bottom:0;width:4px;background:rgba(77,125,85,.2);transform:translateX(-50%)}
/* â”€â”€ Form â”€â”€ */
.finput{display:block;width:100%;padding:.75rem;border-radius:.75rem;border:none;box-shadow:0 0 0 1px rgba(77,125,85,.3);font-family:inherit;font-size:1rem;transition:box-shadow .2s;background:#fff}
.finput:focus{outline:none;box-shadow:0 0 0 2px var(--p)}
label{display:block;font-size:.875rem;font-weight:700;color:var(--s);margin-bottom:.5rem}
/* â”€â”€ Footer â”€â”€ */
.ftr{background:var(--s);color:#fff;padding:4rem 1rem 2rem}
/* â”€â”€ Responsive â”€â”€ */
@media(min-width:768px){
  .dnav{display:flex}
  .mbtn{display:none}
  .g2{display:grid;grid-template-columns:repeat(2,1fr);gap:3rem}
  .g3{display:grid;grid-template-columns:repeat(3,1fr);gap:2rem}
  .g4{display:grid;grid-template-columns:repeat(4,1fr);gap:3rem}
  .tline{display:block}
  .md-tr{text-align:right}
  .md-pr{padding-right:3rem}
  .md-pl{padding-left:3rem}
  .md-hw{width:50%}
}
/* â”€â”€ Color helpers â”€â”€ */
